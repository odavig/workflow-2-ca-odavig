top: 5%;\n  font-family: \"Helvetica Neue\";\n  padding: 3%;\n  border: none;\n}\n\n#footer button {\n  border: none;\n  font-family: \"Helvetica Neue\";\n  padding: 3% 4% 3% 4%;\n  background-color: white;\n  transition: 0.3s;\n  cursor: pointer;\n}\n\n#footer button:hover {\n  background-color: #0B3D91;\n  color: white;\n}\n\n.smIcon {\n  width: 10%;\n  cursor: pointer;\n}\n\n#contact a {\n  text-decoration: none;\n  margin: 2%;\n}\n\n#newsBox {\n  font-size: 80%;\n  width: 100%;\n}\n\n#newsBox h4 {\n  font-weight: lighter;\n  font-size: 18px;\n  letter-spacing: 0.1em;\n}\n\n#newsBox li {\n  list-style: none;\n  font-size: 13px;\n}\n\n#newsBox a {\n  text-decoration: none;\n  font-size: 18px;\n  color: white;\n  letter-spacing: 0.1em;\n}\n\n#newsBoxLink {\n  margin-top: 5%;\n}\n\n#newsBox ul {\n  padding: 0px;\n}\n\n/* Bredere skjermer enn 768px */\n@media (max-width: 768px) {\n  nav a {\n    display: block;\n    line-height: 3em;\n    font-size: 30px;\n    font-family: \"Helvetica\";\n    color: darkgrey;\n  }\n\n  nav {\n    display: none;\n  }\n\n  header {\n    width: 100%;\n    transition: 0.3s;\n    overflow: hidden;\n    right: 0px;\n  }\n\n  #openNav {\n    display: block;\n    font-size: 20px;\n    position: absolute;\n    right: 2%;\n    top: 2%;\n  }\n\n  #closeNav {\n    display: block;\n  }\n\n  .sectionheading {\n    margin-top: 10%;\n  }\n\n  main {\n    margin: 5%;\n  }\n\n}\n\n/* Bredere skjermer enn 1024px */\n@media (min-width: 1024px) {\n}\n"},"lineCount":null}},"error":null,"hash":"3eb4fa8877c56317c3bb718e9f2755ec","cacheData":{"env":{}}}