n}\n\n#logo {\n  width: 70%;\n  margin: 3%;\n  cursor: pointer;\n}\n\nheader nav {\n  grid-column: 5/13;\n  text-align: right; /*DRY some of these styles appear in other files.*/\n  margin-top: 1.5em;\n}\n\nnav a {\n  margin: 2%;\n  color: #bfbfbf;\n  text-decoration: none;\n  transition: 0.3s;\n  font-family: 'Source Sans Pro', sans-serif;\n  font-size: 130%;\n}\n\nnav a:hover {\n  color: #033A92;\n}\n\n#openNav {\n  display: none;\n}\n\n#closeNav {\n  display: none;\n}\n\n\n/********************* main *********************/\nmain {\n  position: absolute;\n  top: 0px;\n  z-index: -1;\n  width: 100%;\n  height: 100vh;\n  font-family: \"Helvetica Neue\";\n}\n\n#contactBackgroundSec {\n  grid-column: 1/7;\n  background-image: url(../media/nasahome.png);\n  text-align: center;\n}\n\n#contactBackgroundSec p {\n  margin-top: 0;\n  letter-spacing: 0.2em;\n  font-weight: lighter;\n  font-size: 20px;\n  line-height: 2.5em;\n}\n\n#contactBackgroundSec ul {\n  list-style-type: none;\n  padding: 0px;\n}\n\n#contactBackgroundSec li {\n  padding: 2%;\n  transition: 0.3s;\n}\n\n#contactBackgroundSec li a{\n  text-decoration: none;\n  color: white;\n  font-weight: lighter;\n  font-size: 20px;\n  letter-spacing: 0.1em;\n}\n\n#contactBackgroundSec li:hover {\n  background-color: #010B19;\n}\n\n.sectionheading {\n  color: #B3B3B3;\n  margin: 0px;\n  font-size: 250%;\n  text-align: center;\n  letter-spacing: 0.3em;\n  height: 200px;\n  margin-top: 20%;\n}\n\n.smIcon {\n  width: 6%;\n  cursor: pointer;\n  vertical-align: middle;\n}\n\n#contactFormSec {\n  grid-column: 8/13;\n  margin-top: 30%;\n  text-align: center;\n}\n\n#contactFormSec input {\n  width: 40%;\n  padding: 2%;\n  font-family: \"Helvetica Neue\";\n  font-size: 18px;\n  font-weight: 300;\n  text-align: center;\n  margin: 5%;\n}\n\n.input {\n  height: 5%;\n}\n\n#messageInput {\n  height: 90px;\n  width: 40%;\n  padding: 2%;\n  margin: 5%;\n  font-family: \"Helvetica Neue\";\n  font-size: 18px;\n  font-weight: 300;\n  text-align: center;\n}\n\n#contactForm button {\n  padding: 2% 5% 2% 5%;\n  background-color: #0B3D91;\n  border: none;\n  color: white;\n  font-weight: 300;\n  font-style: italic;\n  font-size: 20px;\n  margin-top: 5%;\n  cursor: pointer;\n  transition: 0.3s;\n}\n\n#contactForm button:hover {\n  background-color: #093177;\n}\n\n#contactForm p {\n  margin: 0px;\n  font-size: 15px;\n  font-style: italic;\n  color: #FF0000;\n}\n\n.error {\n  display: none;\n}\n\nlabel {\n  font-family: \"Helvetica Neue\";\n  font-weight: lighter;\n  font-size: 22px;\n}\n\n/* Bredere skjermer enn 768px */\n@media (max-width: 768px) {\n  nav a {\n    display: block;\n    line-height: 3em;\n    font-size: 30px;\n    font-family: \"Helvetica\";\n    color: darkgrey;\n  }\n\n  nav {\n    display: none;\n  }\n\n  header {\n    width: 100%;\n    transition: 0.3s;\n    overflow: hidden;\n    right: 0px;\n  }\n\n  #openNav {\n    display: block;\n    font-size: 20px;\n    position: absolute;\n    right: 2%;\n    top: 2%;\n  }\n\n  #closeNav {\n    display: block;\n  }\n\n  #main {\n    display: block;\n  }\n\n  #contactFormSec {\n    margin: 10%;\n  }\n\n  .sectionheading {\n    height: auto;\n    margin: 5%;\n  }\n\n}\n\n/* Bredere skjermer enn 1024px */\n@media (min-width: 1024px) {\n}\n"},"lineCount":null}},"error":null,"hash":"7da8c91e6d97139209325774027457f0","cacheData":{"env":{}}}